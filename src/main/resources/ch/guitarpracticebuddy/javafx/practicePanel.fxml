<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<VBox id="VBox" alignment="CENTER" spacing="5.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="ch.guitarpracticebuddy.javafx.PracticeController">
    <children>
        <BorderPane prefHeight="629.0" prefWidth="1034.0" VBox.vgrow="ALWAYS">
            <center>
                <VBox fx:id="practiceContentPanel" alignment="CENTER" prefHeight="200.0" prefWidth="100.0"
                      BorderPane.alignment="CENTER">
                    <BorderPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </BorderPane.margin>
                </VBox>
            </center>
            <left>
                <VBox id="VBox" alignment="CENTER" spacing="5.0" BorderPane.alignment="TOP_LEFT">
                    <children>
                        <AnchorPane prefHeight="101.0" prefWidth="200.0">
                            <children>
                                <ProgressIndicator fx:id="progressBar" layoutY="33.0" prefHeight="54.0" prefWidth="57.0"
                                                   progress="0.0"/>
                                <HBox alignment="CENTER" prefHeight="27.0" prefWidth="200.0" spacing="5.0">
                                    <children>
                                        <Button id="playButton" fx:id="startButton" maxWidth="1.7976931348623157E308"
                                                minWidth="-Infinity" mnemonicParsing="false" prefWidth="60.0"
                                                text="Play" HBox.hgrow="NEVER"/>
                                        <Button fx:id="skipButton" maxWidth="-Infinity" minWidth="-Infinity"
                                                mnemonicParsing="false" prefWidth="60.0" text="Skip"
                                                HBox.hgrow="NEVER"/>
                                        <Button fx:id="resetButton" maxWidth="-Infinity" minWidth="-Infinity"
                                                mnemonicParsing="false" prefWidth="60.0" text="Reset"
                                                HBox.hgrow="NEVER">
                                            <HBox.margin>
                                                <Insets/>
                                            </HBox.margin>
                                        </Button>
                                    </children>
                                </HBox>
                                <Slider fx:id="bpmSlider" layoutX="57.0" layoutY="35.0" prefWidth="90.0"/>
                                <ChoiceBox fx:id="ratingBox" layoutX="57.0" layoutY="60.0" prefWidth="129.0">
                                    <items>
                                        <FXCollections fx:factory="observableArrayList">
                                            <String fx:value="Item 1"/>
                                            <String fx:value="Item 2"/>
                                            <String fx:value="Item 3"/>
                                        </FXCollections>
                                    </items>
                                </ChoiceBox>
                                <ToggleButton fx:id="bpmButton" graphicTextGap="2.0" layoutX="147.0" layoutY="31.0"
                                              minWidth="33.0" mnemonicParsing="false" prefWidth="47.999900000002526"
                                              textOverrun="CLIP"/>
                            </children>
                        </AnchorPane>
                        <ListView fx:id="currentExercisesTable" prefHeight="200.0" prefWidth="200.0"
                                  VBox.vgrow="ALWAYS"/>
                        <Pane fx:id="playerPane" prefHeight="100.0" prefWidth="200.0"/>
                    </children>
                </VBox>
            </left>
        </BorderPane>
    </children>
</VBox>
