<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<HBox id="HBox" alignment="CENTER" spacing="5.0" xmlns:fx="http://javafx.com/fxml" fx:controller="ch.guitarpracticebuddy.javafx.TagAdminController">
  <children>
    <ListView fx:id="tagAdminList" prefHeight="400.0" prefWidth="422.0" />
    <VBox prefHeight="393.0" prefWidth="93.0">
      <children>
        <TextField fx:id="tagAdminNameField" prefWidth="200.0" />
        <Button fx:id="tagAdminNewButton" mnemonicParsing="false" prefWidth="200.0" text="New" VBox.vgrow="NEVER" />
        <Button fx:id="tagAdminEditButton" mnemonicParsing="false" prefWidth="200.0" text="Edit" VBox.vgrow="NEVER" />
        <Button fx:id="tagAdminDeleteButton" mnemonicParsing="false" prefWidth="200.0" text="Delete" VBox.vgrow="NEVER" />
      </children>
    </VBox>
  </children>
</HBox>
